pub type Int
pub type String

pub type Unit =
    | Unit

pub type Bool =
    | True
    | False

pub type Result ok err =
    | Ok ok
    | Err err

pub external add : Int -> Int -> Int = "add"

pub external pipe : forall a b. a -> (a -> b) -> b = "($0 + $1)"

pub external log : String -> Int = "console.log"
